p>{{ user }}</p>\r\n  <p>{{ post }}</p>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: \"publication\",\r\n  data() {\r\n    return {\r\n      publication: {\r\n        id: \"8\",\r\n        content: \"azezaee\",\r\n        userId: \"14\"\r\n      },\r\n      user: [],\r\n      post: {\r\n        id: this.post,\r\n        content: \"azezaee\",\r\n        userId: \"14\"\r\n      }\r\n\r\n\r\n    };\r\n  },\r\n  methods: {\r\n    async mounted() {\r\n      const res = await fetch('http://localhost:3000/api/publication/get')\r\n      const data = await res.json()\r\n      this.publication = data\r\n      console.log(data)\r\n    },\r\n    testClick() {\r\n      const test = console.log(\"bonjours\")\r\n      return test\r\n    },\r\n\r\n    loadUsers() {\r\n      axios.get('http://localhost:3000/api/publication/get')\r\n        .then(function (response) {\r\n          this.post = response.data;\r\n          return this.post\r\n        }.bind(this));\r\n    }\r\n  }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n.well {\r\n  margin-top: 50px;\r\n  background-color: white;\r\n}\r\n\r\n.main-post {\r\n  display: flex;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}