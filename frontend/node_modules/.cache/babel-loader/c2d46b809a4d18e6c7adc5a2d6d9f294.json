{"ast":null,"code":"import HeaderMainPage from \"@/components/HeaderMainPage.vue\";\nexport default {\n  components: {\n    HeaderMainPage\n  },\n\n  data() {\n    return {\n      publications: \"\"\n    };\n  },\n\n  computed: {\n    publicationId() {\n      return parseInt(this.$route.params.id);\n    }\n\n  },\n\n  async created() {\n    fetch(`http://localhost:3000/api/publications/`).then(response => response.json()).then(data => {\n      console.log(data);\n      const postId = this.$route.params.id;\n      const MainPost = data.find(post => post.id == postId);\n      this.publications = MainPost;\n      console.log(MainPost);\n    });\n  },\n\n  SendComment() {\n    const postId = this.$route.params.id;\n    fetch('http://localhost:3000/api/comments/', {\n      method: \"POST\",\n      headers: {\n        'Accept': 'application/json',\n        'Content-Type': 'application/json',\n        'Authorization': \"Bearer \" + sessionStorage.getItem(\"Token\")\n      },\n      body: {\n        publicationId: postId,\n        commentaire: \"test\"\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AA0DA,OAAOA,cAAP,MAA2B,iCAA3B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKbE,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE;IADT,CAAP;EAGD,CATY;;EAUbC,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAOC,QAAQ,CAAC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAApB,CAAf;IACF;;EAHQ,CAVG;;EAiBb,MAAMC,OAAN,GAAgB;IACdC,KAAK,CAAE,yCAAF,CAAL,CACGC,IADH,CACQC,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADpB,EAEGF,IAFH,CAEQV,IAAG,IAAK;MACZa,OAAO,CAACC,GAAR,CAAYd,IAAZ;MACA,MAAMe,MAAK,GAAI,KAAKV,MAAL,CAAYC,MAAZ,CAAmBC,EAAlC;MACA,MAAMS,QAAO,GAAIhB,IAAI,CAACiB,IAAL,CAAUC,IAAG,IAAKA,IAAI,CAACX,EAAL,IAAWQ,MAA7B,CAAjB;MACA,KAAKd,YAAL,GAAoBe,QAApB;MACAH,OAAO,CAACC,GAAR,CAAYE,QAAZ;IAGD,CAVH;EAYD,CA9BY;;EA+BbG,WAAW,GAAG;IACZ,MAAMJ,MAAK,GAAI,KAAKV,MAAL,CAAYC,MAAZ,CAAmBC,EAAlC;IACAE,KAAK,CAAC,qCAAD,EAAwC;MAC3CW,MAAM,EAAE,MADmC;MAE3CC,OAAO,EAAE;QACP,UAAU,kBADH;QAEP,gBAAgB,kBAFT;QAGP,iBAAiB,YAAYC,cAAc,CAACC,OAAf,CAAuB,OAAvB;MAHtB,CAFkC;MAO3CC,I