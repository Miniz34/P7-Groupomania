{"ast":null,"code":"export default {\n  name: \"Login-Template\",\n\n  data() {\n    return {\n      dataLogin: {\n        username: \"\",\n        password: \"\",\n        avatar: \"\"\n      },\n      isVisibleRegex: false,\n      isVisibleEmpty: false\n    };\n  },\n\n  methods: {\n    mounted() {\n      const regexPassword = /^[a-zA-Z0-9_-]{3,16}$/;\n\n      if (this.dataLogin.username && this.dataLogin.password && regexPassword.test(this.dataLogin.password)) {\n        fetch(\"http://localhost:3000/api/users/auth\", {\n          method: \"POST\",\n          body: JSON.stringify({\n            username: this.dataLogin.username,\n            password: this.dataLogin.password,\n            avatar: this.dataLogin.avatar\n          }),\n          headers: {\n            'Accept': 'application/json',\n            'Content-Type': 'application/json'\n          }\n        }).then(response => response.json()); // .then(document.location.href = `http://localhost:8080/publications`\n        // )\n      } else {\n        if (this.dataLogin.username && this.dataLogin.password && regexPassword.test(this.dataLogin.password) === false) {\n          this.isVisibleRegex = true;\n        } else {\n          this.isVisibleEmpty = true;\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmCA,eAAe;EACbA,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,QAAQ,EAAE,EADD;QAETC,QAAQ,EAAE,EAFD;QAGTC,MAAM,EAAE;MAHC,CADN;MAMLC,cAAc,EAAE,KANX;MAOLC,cAAc,EAAE;IAPX,CAAP;EAUD,CAbY;;EAaVC,OAAO,EAAE;IACVC,OAAO,GAAG;MAGR,MAAMC,aAAY,GAAI,uBAAtB;;MACA,IAAI,KAAKR,SAAL,CAAeC,QAAf,IAA2B,KAAKD,SAAL,CAAeE,QAA1C,IAAsDM,aAAa,CAACC,IAAd,CAAmB,KAAKT,SAAL,CAAeE,QAAlC,CAA1D,EAAuG;QACrGQ,KAAK,CAAC,sCAAD,EAAyC;UAC5CC,MAAM,EAAE,MADoC;UAE5CC,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;YACnBb,QAAQ,EAAE,KAAKD,SAAL,CAAeC,QADN;YAEnBC,QAAQ,EAAE,KAAKF,SAAL,CAAeE,QAFN;YAGnBC,MAAM,EAAE,KAAKH,SAAL,CAAeG;UAHJ,CAAf,CAFsC;UAO5CY,OAAO,EAAE;YACP,UAAU,kBADH;YAEP,gBAAgB;UAFT;QAPmC,CAAzC,CAAL,CAYGC,IAZH,CAYSC