{"ast":null,"code":"// import moment from 'moment'\nimport HeaderMainPage from \"@/components/HeaderMainPage.vue\";\nexport default {\n  components: {\n    HeaderMainPage\n  },\n\n  data() {\n    return {\n      publications: \"\",\n      comment: \"\"\n    };\n  },\n\n  computed: {\n    publicationId() {\n      return parseInt(this.$route.params.id);\n    }\n\n  },\n\n  async created() {\n    const moment = require('moment');\n\n    console.log(moment);\n    fetch(`http://localhost:3000/api/publications/`).then(response => response.json()).then(data => {\n      console.log(data);\n      const postId = this.$route.params.id;\n      const MainPost = data.find(post => post.id == postId);\n      this.publications = MainPost;\n      console.log(MainPost);\n      MainPost.createdAt.format('Do-MMM-YYYY');\n    });\n  },\n\n  methods: {\n    SendComment() {\n      const moment = require('moment');\n\n      const aujourdhui = moment();\n      console.log(\"hello\");\n      const postId = this.$route.params.id;\n      fetch('http://localhost:3000/api/comments/', {\n        method: \"POST\",\n        headers: {\n          'Accept': 'application/json',\n          'Content-Type': 'application/json' // 'Authorization': \"Bearer \" + sessionStorage.getItem(\"Token\")\n\n        },\n        body: JSON.stringify({\n          publicationId: this.publications.id,\n          commentaire: this.comment\n        })\n      });\n      console.log(postId);\n      console.log(aujourdhui.format('Do-MMM-YYYY'));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DA;AAEA,OAAOA,cAAP,MAA2B,iCAA3B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKbE,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,EADT;MAELC,OAAO,EAAE;IAFJ,CAAP;EAID,CAVY;;EAWbC,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAOC,QAAQ,CAAC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAApB,CAAf;IACD;;EAHO,CAXG;;EAoBb,MAAMC,OAAN,GAAgB;IACd,MAAMC,MAAK,GAAIC,OAAO,CAAC,QAAD,CAAtB;;IACAC,OAAO,CAACC,GAAR,CAAYH,MAAZ;IAEAI,KAAK,CAAE,yCAAF,CAAL,CACGC,IADH,CACQC,QAAO,IAAKA,QAAQ,CAACC,IA