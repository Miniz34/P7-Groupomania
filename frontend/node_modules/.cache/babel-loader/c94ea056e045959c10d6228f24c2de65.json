{"ast":null,"code":"import axios from 'axios'; // import { json } from 'body-parser';\n\nconst userId = sessionStorage.getItem(\"userId\");\nexport default {\n  name: \"newPost\",\n\n  data() {\n    return {\n      dataPost: {\n        title: \"\",\n        content: \"\",\n        comment: \"\"\n      }\n    };\n  },\n\n  methods: {\n    mounted() {\n      const fd = new FormData();\n      fd.append(\"title\", this.dataPost.title);\n      fd.append(\"content\", this.dataPost.content);\n      fd.append(\"inputFile\", this.dataPost.image);\n      fd.append(\"userId\", userId);\n      console.log(\"test récup\", fd.get(\"title\"));\n      console.log(\"test récup\", fd.get(\"content\"));\n      console.log(\"test récup\", fd.get(\"inputFile\"));\n      console.log(\"test récup\", fd.get(\"userId\"));\n\n      if (this.dataPost.title && this.dataPost.content) {\n        const postId = this.$route.params.id;\n        axios.put(\"http://localhost:3000/api/publications/\" + postId, fd, {}).then(response => {\n          console.log(response);\n        });\n      }\n    },\n\n    onFileChange(e) {\n      console.log(e);\n      this.dataPost.image = e.target.files[0] || e.dataTransfer.files;\n      console.log(this.dataPost.image);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,MAAMC,MAAK,GAAIC,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAAf;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,KAAK,EAAE,EADC;QAERC,OAAO,EAAE,EAFD;QAGRC,OAAO,EAAE;MAHD;IADL,CAAP;EAQD,CAXY;;EAYbC,OAAO,EAAE;IAEPC,OAAO,GAAG;MAER,MAAMC,EAAC,GAAI,IAAIC,QAAJ,EAAX;MACAD,EAAE,CAACE,MAAH,CAAU,OAAV,EAAmB,KAAKR,QAAL,CAAcC,KAAjC;MACAK,EAAE,CAACE,MAAH,CAAU,SAAV,EAAqB,KAAKR,QAAL,CAAcE,OAAnC;MACAI,EAAE,CAACE,MAAH,CAAU,WAAV,EAAuB,KAAKR,QAAL,CAAcS,KAArC;MACAH,EAAE,CAACE,MAAH,CAAU,QAAV,EAAoBb,MAApB;MAEAe,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BL,EAAE,CAACM,GAAH,CAAO,OAAP,CAA1B;MACAF,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BL,EAAE,CAACM,GAAH,CAAO,SAAP,CAA1B;MACAF,OAAO,CAACC,GA