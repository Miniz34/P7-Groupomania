{"ast":null,"code":"// import axios from 'axios';\n// import { json } from 'body-parser';\nconst id = sessionStorage.getItem(\"userId\");\nexport default {\n  name: \"newPost\",\n\n  data() {\n    return {\n      dataPost: {\n        title: \"\",\n        content: \"\",\n        comment: \"\"\n      }\n    };\n  },\n\n  methods: {\n    mounted() {\n      const fd = new FormData();\n      fd.append(\"title\", this.dataPost.title);\n      fd.append(\"content\", this.dataPost.content);\n      fd.append(\"inputFile\", this.dataPost.image);\n      fd.append(\"id\", id);\n      console.log(\"test récup\", fd.get(\"title\"));\n      console.log(\"test récup\", fd.get(\"content\"));\n      console.log(\"test récup\", fd.get(\"inputFile\"));\n      console.log(\"test récup\", fd.get(\"id\"));\n\n      if (this.dataPost.title && this.dataPost.content) {\n        fetch(\"http://localhost:3000/api/publications\", {\n          method: \"POST\",\n          body: JSON.stringify({\n            fd\n          })\n        }).then(response => {\n          console.log(response);\n        });\n      }\n    },\n\n    onFileChange(e) {\n      console.log(e);\n      this.dataPost.image = e.target.files[0] || e.dataTransfer.files;\n      console.log(this.dataPost.image);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA;AACA;AACA,MAAMA,EAAC,GAAIC,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAAX;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,KAAK,EAAE,EADC;QAERC,OAAO,EAAE,EAFD;QAGRC,OAAO,EAAE;MAHD;IADL,CAAP;EAQD,CAXY;;EAYbC,OAAO,EAAE;IACPC,OAAO,GAAG;MACR,MAAMC,EAAC,GAAI,IAAIC,QAAJ,EAAX;MACAD,EAAE,CAACE,MAAH,CAAU,OAAV,EAAmB,KAAKR,QAAL,CAAcC,KAAjC;MACAK,EAAE,CAACE,MAAH,CAAU,SAAV,EAAqB,KAAKR,QAAL,CAAcE,OAAnC;MACAI,EAAE,CAACE,MAAH,CAAU,WAAV,EAAuB,KAAKR,QAAL,CAAcS,KAArC;MACAH,EAAE,CAACE,MAAH,CAAU,IAAV,EAAgBb,EAAhB;MAEAe,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BL,EAAE,CAACM,GAAH,CAAO,OAAP,CAA1B;MACAF,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BL,EAAE,CAACM,GAAH,CAAO,SAAP,CAA1B;MACAF,OAAO,CAACC,GAAR