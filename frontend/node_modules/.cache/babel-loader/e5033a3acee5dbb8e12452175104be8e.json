{"ast":null,"code":"// import axios from 'axios';\nexport default {\n  name: \"Login-Template\",\n\n  data() {\n    return {\n      dataLogin: {\n        firstname: \"\",\n        lastname: \"\",\n        email: \"\",\n        password: \"\",\n        id: \"\"\n      },\n      isVisibleRegex: false,\n      isVisibleEmpty: false\n    };\n  },\n\n  methods: {\n    mounted() {\n      const regexPassword = /^[a-zA-Z0-9_-]{3,16}$/;\n\n      if (this.dataLogin.email && this.dataLogin.password && this.dataLogin.firstname && this.dataLogin.lastname && regexPassword.test(this.dataLogin.password) && this.dataLogin.password == this.dataLogin.repeatPassword) {\n        fetch(\"http://localhost:3000/api/users/auth\", {\n          method: \"POST\",\n          body: JSON.stringify({\n            firstname: this.dataLogin.firstname,\n            lastname: this.dataLogin.lastname,\n            email: this.dataLogin.email,\n            password: this.dataLogin.password\n          }),\n          headers: {\n            'Accept': 'application/json',\n            'Content-Type': 'application/json' // 'Content-Type': 'multipart/form-data'\n\n          }\n        }).then(response => response.json()); // .then(() =>\n        //   document.location.href = `http://localhost:8080/login`)\n        // )\n      } else {\n        if (this.dataLogin.firstname && this.dataLogin.lastname && this.dataLogin.email && this.dataLogin.password && regexPassword.test(this.dataLogin.password) === false) {\n          this.isVisibleRegex = true;\n        } else {\n          this.isVisibleEmpty = true;\n        }\n      }\n    },\n\n    onFileChange(e) {\n      console.log(e);\n      this.dataLogin.avatar = e.target.files[0] || e.dataTransfer.files;\n      console.log(this.dataLogin.avatar);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA;AAEA,eAAe;EACbA,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,SAAS,EAAE,EADF;QAETC,QAAQ,EAAE,EAFD;QAGTC,KAAK,EAAE,EAHE;QAITC,QAAQ,EAAE,EAJD;QAKTC,EAAE,EAAE;MALK,CADN;MAQLC,cAAc,EAAE,KARX;MASLC,cAAc,EAAE;IATX,CAAP;EAYD,CAfY;;EAeVC,OAAO,EAAE;IAEVC,OAAO,GAAG;MAER,MAAMC,aAAY,GAAI,uBAAtB;;MACA,IAAI,KAAKV,SAAL,CAAeG,KAAf,IAAwB,KAAKH,SAAL,CAAeI,QAAvC,IAAmD,KAAKJ,SAAL,CAAeC,SAAlE,IAA+E,KAAKD,SAAL,CAAeE,QAA9F,IAA0GQ,aAAa,CAACC,IAAd,CAAmB,KAAKX,SAAL,CAAeI,QAAlC,CAA1G,IAAyJ,KAAKJ,SAAL,CAAeI,QAAf,IAA2B,KAAKJ,SAAL,CAAeY,cAAvM,EAAuN;QAErNC,KAAK,CAAC,sCAAD,EAAyC;UAC5CC,MAAM,EAAE,MADoC;UAE5CC,IAAI,EACFC,IAAI,CAACC,SAAL,CAAe;YACbhB,SAAS,EAAE,KAAKD,SAAL,CAAeC,SADb;YAEbC,QAAQ,EAAE,KAAKF,SAAL,CAAeE,QAFZ;YAGbC,KAAK,EAAE,KAAKH,SAAL,CAAeG,KAHT;YAIbC,QAAQ,EAAE,KAAKJ,SAAL,CAAeI;UAJZ,CAAf,CAH0C;UAS5Cc,OAAO,EAAE;YACP,UAAU,kBADH;YAEP,gBAAgB,kBAFT,CAGP;;UAHO;QATmC,CAAzC,CAAL,CAeGC,IAfH,CAeSC,QAAD,IAAcA,QAAQ,CAACC,IAAT,EAftB,EAFqN,CAkBrN;QACA;QACA;MAEF,CAtBA,MAsBO;QACL,IAAI,KAAKrB,SAAL,CAAeC,SAAf,IAA4B,KAAKD,SAAL,CAAeE,QAA3C,IAAuD,KAAKF,SAAL,CAAeG,KAAtE,IAA+E,KAAKH,SAAL,CAAeI,QAA9F,IAA0GM,aAAa,CAACC,IAAd,CAAmB,KAAKX,SAAL,CAAeI,QAAlC,MAAgD,KAA9J,EAAqK;UACnK,KAAKE,cAAL,GAAsB,IAAtB;QAEF,CAHA,MAGO;UACL,KAAKC,cAAL,GAAsB,IAAtB;QAEF;MAEF;IACD,CArCS;;IAsCVe,YAAY,CAACC,CAAD,EAAI;MACdC,OAAO,CAACC,GAAR,CAAYF,CAAZ;MACA,KAAKvB,SAAL,CAAe0B,MAAf,GAAwBH,CAAC,CAACI,MAAF,CAASC,KAAT,CAAe,CAAf,KAAqBL,CAAC,CAACM,YAAF,CAAeD,KAA5D;MACAJ,OAAO,CAACC,GAAR,CAAY,KAAKzB,SAAL,CAAe0B,MAA3B;IAED;;EA3CS;AAfC,CAAf","names":["name","data","dataLogin","firstname","lastname","email","password","id","isVisibleRegex","isVisibleEmpty","methods","mounted","regexPassword","test","repeatPassword","fetch","method","body","JSON","stringify","headers","then","response","json","onFileChange","e","console","log","avatar","target","files","dataTransfer"],"sourceRoot":"","sources":["D:\\Code\\Projet 7\\p7-groupomania\\P7-Groupomania\\frontend\\src\\components\\AuthTemplate.vue"],"sourcesContent":[" <template>\r\n\r\n\r\n\r\n  <form id=\"Auth-View\" enctype=\"multipart/form-data\" method=\"post\">\r\n\r\n    <section class=\"d-flex flex-column main-form w-50 align-center main-form\">\r\n      <h2 class=\"col align-self-center\">Inscription</h2>\r\n      <label for=\"firstname\" class=\"col-md-4 col-form-label text-md-right\">First Name</label>\r\n      <input v-model=\"dataLogin.firstname\" class=\"form-control form-control-lg\" type=\"text\" placeholder=\"firstname\"\r\n        id=\"firstname\">\r\n      <label for=\"lastname\" class=\"col-md-4 col-form-label text-md-right\">Last Name</label>\r\n      <input v-model=\"dataLogin.lastname\" class=\"form-control form-control-lg\" type=\"text\" placeholder=\"lastname\"\r\n        id=\"lastname\">\r\n\r\n      <label for=\"email\" class=\"col-md-4 col-form-label text-md-right\">Email</label>\r\n      <input v-model=\"dataLogin.email\" class=\"form-control form-control-lg\" type=\"email\" placeholder=\"email\" id=\"email\">\r\n\r\n      <label for=\"password\" class=\"col-md-4 col-form-label text-md-right\">Password</label>\r\n      <input v-model=\"dataLogin.password\" class=\" form-control form-control-lg\" type=\"text\" placeholder=\"Password\"\r\n        id=\"password\">\r\n      <label for=\"password\" class=\"col-md-4 col-form-label text-md-right\">Repeat Password</label>\r\n      <input v-model=\"dataLogin.password\" class=\" form-control form-control-lg\" type=\"text\" placeholder=\"Password\"\r\n        id=\"password\">\r\n\r\n      <!-- <div class=\"custom-file\">\r\n        <input name=\"inputFile\" type=\"file\" accept=\"image/*\" class=\"custom-file-input\" id=\"inputFile\"\r\n          @change=\"onFileChange\" />\r\n        <label class=\"custom-file-label\" for=\"inputFile\">Choose file</label>\r\n        <button @click=\"onSubmit\">Upload</button>\r\n      </div> -->\r\n\r\n      <div v-if=\"isVisibleRegex\" class=\"alert alert-danger\" role=\"alert\">\r\n        Votre mot de passe doit avoir entre 3 et 16 caractères, seules les lettres, chiffres et tirets sont autorisés\r\n      </div>\r\n      <div v-if=\"isVisibleEmpty\" class=\"alert alert-danger\" role=\"alert\">\r\n        Veuillez remplir les champs \"Prénom\", \"Nom\", \"Email\" et \"Password\"\r\n      </div>\r\n      <!-- <div class=\"valid-feedback\">\r\n        Looks good!\r\n      </div> -->\r\n\r\n      <button @click.prevent=\"mounted\" type=\"submit\"\r\n        class=\"btn btn-primary w-25 col align-self-center\">S'inscrire</button>\r\n      <p class=\"col align-self-center\">Déjà inscrit ? Vous pouvez vous connecter <a href=\"/login\">ici</a></p>\r\n    </section>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\n\r\n// import axios from 'axios';\r\n\r\nexport default {\r\n  name: \"Login-Template\",\r\n  data() {\r\n    return {\r\n      dataLogin: {\r\n        firstname: \"\",\r\n        lastname: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        id: \"\"\r\n      },\r\n      isVisibleRegex: false,\r\n      isVisibleEmpty: false\r\n\r\n    };\r\n  }, methods: {\r\n\r\n    mounted() {\r\n\r\n      const regexPassword = /^[a-zA-Z0-9_-]{3,16}$/;\r\n      if (this.dataLogin.email && this.dataLogin.password && this.dataLogin.firstname && this.dataLogin.lastname && regexPassword.test(this.dataLogin.password) && this.dataLogin.password == this.dataLogin.repeatPassword) {\r\n\r\n        fetch(\"http://localhost:3000/api/users/auth\", {\r\n          method: \"POST\",\r\n          body:\r\n            JSON.stringify({\r\n              firstname: this.dataLogin.firstname,\r\n              lastname: this.dataLogin.lastname,\r\n              email: this.dataLogin.email,\r\n              password: this.dataLogin.password\r\n            }),\r\n          headers: {\r\n            'Accept': 'application/json',\r\n            'Content-Type': 'application/json',\r\n            // 'Content-Type': 'multipart/form-data'\r\n          }\r\n        })\r\n          .then((response) => response.json())\r\n        // .then(() =>\r\n        //   document.location.href = `http://localhost:8080/login`)\r\n        // )\r\n\r\n      } else {\r\n        if (this.dataLogin.firstname && this.dataLogin.lastname && this.dataLogin.email && this.dataLogin.password && regexPassword.test(this.dataLogin.password) === false) {\r\n          this.isVisibleRegex = true\r\n\r\n        } else {\r\n          this.isVisibleEmpty = true\r\n\r\n        }\r\n\r\n      }\r\n    },\r\n    onFileChange(e) {\r\n      console.log(e);\r\n      this.dataLogin.avatar = e.target.files[0] || e.dataTransfer.files;\r\n      console.log(this.dataLogin.avatar);\r\n\r\n    },\r\n\r\n  }\r\n}\r\n\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n.btn {\r\n\r\n  margin-top: 20px;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n\r\n\r\n<!-- \r\n<div class=\"form-group\">\r\n    <input type=\"file\" accept=\"image/*\" #imageInput (change)=\"onFileAdded($event)\">\r\n    <button mat-raised-button color=\"primary\" (click)=\"imageInput.click()\">ADD IMAGE</button>\r\n    <img [src]=\"imagePreview\" *ngIf=\"imagePreview\" style=\"max-height: 100px;display:block;margin-top:10px\">\r\n  </div> -->"]},"metadata":{},"sourceType":"module"}