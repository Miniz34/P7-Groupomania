ost\">\r\n\r\n              <!-- <span><i class=\"glyphicon glyphicon-comment\"></i> 2 comments</span> -->\r\n              <p>{{ }} </p>\r\n\r\n\r\n              <p>\r\n\r\n              </p>\r\n\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- <div v-for=\"comment in publications.comments\" :key=\"comment.id\" class=\"Comments-View\">\r\n      <div class=\"comment-data d-flex justify-content-start\">\r\n        <div class=\"username\">\r\n          {{ comment.id }}\r\n\r\n        </div>\r\n        <div class=\"date\">\r\n          {{ comment.createdAt }}\r\n        </div>\r\n      </div>\r\n      <div class=\"comment\">\r\n        {{ comment.commentaire }}\r\n      </div>\r\n    </div>\r\n     <p>\r\n      {{ publications.comments }}\r\n    </p> -->\r\n\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport HeaderMainPage from \"@/components/HeaderMainPage.vue\"\r\n\r\nexport default {\r\n  components: {\r\n    HeaderMainPage\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      publications: \"\",\r\n    }\r\n  },\r\n  computed: {\r\n    publicationId() {\r\n      return parseInt(this.$route.params.id)\r\n    }\r\n\r\n  },\r\n\r\n  async created() {\r\n    fetch(`http://localhost:3000/api/publications/`)\r\n      .then(response => response.json())\r\n      .then(data => {\r\n        console.log(data)\r\n        const postId = this.$route.params.id;\r\n        const MainPost = data.find(post => post.id == postId)\r\n        this.publications = MainPost\r\n        console.log(MainPost)\r\n\r\n\r\n      })\r\n\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.date {\r\n  margin-left: 25px\r\n}\r\n\r\n.comment {\r\n  margin-bottom: 20px\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}