   </div>\r\n          <textarea v-model=\"dataPost.content\" id=\"editor\" cols=\"30\" rows=\"10\">Submit your text post here...</textarea>\r\n          <br>\r\n\r\n          <div class=\"custom-file\">\r\n            <input name=\"inputFile\" type=\"file\" class=\"custom-file-input\" id=\"inputFile\"\r\n              aria-describedby=\"inputFileAddon\" @change=\"onFileChange\" />\r\n            <label class=\"custom-file-label\" for=\"inputFile\">Choose file</label>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <button @click=\"mounted\" class=\"btn btn-primary\" id=\"submit\">Submit new post</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </form>\r\n\r\n\r\n  <button @click=\"test\">test</button>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nconst id = sessionStorage.getItem(\"userId\")\r\n\r\nexport default {\r\n  name: \"newPost\",\r\n  data() {\r\n    return {\r\n      dataPost: {\r\n        title: \"\",\r\n        content: \"\",\r\n        comment: \"\",\r\n        // image: \"\",\r\n\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n\r\n    mounted() {\r\n      const fd = new FormData();\r\n      fd.append(\"title\", this.dataPost.title);\r\n      fd.append(\"content\", this.dataPost.content);\r\n      fd.append(\"image\", this.dataPost.image);\r\n      console.log(\"test récup\", fd.get(\"title\"));\r\n      console.log(\"test récup\", fd.get(\"content\"));\r\n      console.log(\"test récup\", fd.get(\"image\"));\r\n      if (this.dataPost.title && this.dataPost.content) {\r\n        fetch(\"http://localhost:3000/api/publications\", {\r\n          method: \"POST\",\r\n          body: JSON.stringify({\r\n            title: this.dataPost.title,\r\n            content: this.dataPost.content,\r\n            userId: id,\r\n\r\n          }),\r\n          headers: {\r\n            'Accept': 'application/json',\r\n            'Content-Type': 'application/json',\r\n            'Authorization': \"Bearer \" + sessionStorage.getItem(\"Token\")\r\n\r\n          }\r\n        }).then(function (response) {\r\n\r\n          if (response.ok) {\r\n            // document.location.href = `http://localhost:8080/publications`;\r\n            console.log(fd)\r\n\r\n          }\r\n        })\r\n      }\r\n    },\r\n    onFileChange(e) {\r\n      console.log(e);\r\n      this.dataPost.image = e.target.files[0] || e.dataTransfer.files;\r\n      console.log(this.dataPost.image);\r\n\r\n    },\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n <!-- const fd = new FormData();\r\n        fd.append(\"inputFile\", this.dataLogin.avatar);\r\n        fd.append(\"username\", this.dataLogin.username);\r\n        fd.append(\"password\", this.dataLogin.password);\r\n        console.log(\"test récup\", fd.get(\"inputFile\"));\r\n        console.log(\"test récup\", fd.get(\"username\"));\r\n        console.log(\"test récup\", fd.get(\"password\")); -->"]},"metadata":{},"sourceType":"module"}