t = this.publications.user.userId\r\n      let idPost = this.publications.userId\r\n      if (newId == idPost) {\r\n        // postCreator = true\r\n        // console.log(postCreator)\r\n        console.log(newId)\r\n        console.log(idPost)\r\n        return true\r\n\r\n\r\n\r\n\r\n      } else {\r\n        // postCreator = false\r\n        // console.log(postCreator)\r\n        console.log(newId)\r\n        console.log(idPost)\r\n        return false\r\n\r\n\r\n      }\r\n    }\r\n  },\r\n\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.date {\r\n  margin-left: 25px\r\n}\r\n\r\n.comment {\r\n  margin-bottom: 20px\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}